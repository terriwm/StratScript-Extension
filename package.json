{
  "name": "stratscript-language",
  "displayName": "StratScript Language",
  "description": "Syntax highlighting and autocomplete for 41103T's implementation of StratScript",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "stratscript",
      "aliases": ["StratScript", "stratscript"],
      "extensions": [".strats"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "stratscript",
      "scopeName": "strategies.strats",
      "path": "./syntaxes/stratscript.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "stratscript",
        "path": "./snippets.json"
      }
    ]
  }
}
